{
  "name": "chronoflow",
  "version": "2.0.0",
  "description": "ChronoFlow - A sleek Chrome extension for effortless time tracking and productivity insights",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write src/**/*.{js,ts,html,css}",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "fake-indexeddb": "^6.2.0",
    "html-webpack-plugin": "^5.6.4",
    "jest": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "jest-webextension-mock": "^4.0.0",
    "mini-css-extract-plugin": "^2.9.4",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "keywords": [
    "chrome-extension",
    "time-tracker",
    "productivity"
  ],
  "author": "Mustafa Zidan <mustafa.zidan@gmail.com>, Mohamed Emad <emadhegab@gmail.com>",
  "license": "BSD-3-Clause"
}
